{
  "name": "tanner-start",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "yarn@4.9.4",
  "engines": {
    "node": ">=22.12.0",
    "yarn": ">=4.0.0"
  },
  "scripts": {
    "preinstall": "npx only-allow yarn",
    "dev": "vite",
    "start": "node start.js",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "prepare": "husky",
    "lint": "biome lint --config-path=biome.dev.json .",
    "lint:prod": "biome lint --config-path=biome.prod.json .",
    "lint:fix": "biome lint --config-path=biome.dev.json --write .",
    "lint:fix:prod": "biome lint --config-path=biome.prod.json --write .",
    "format": "biome format --config-path=biome.dev.json --write .",
    "format:prod": "biome format --config-path=biome.prod.json --write .",
    "format:check": "biome format --config-path=biome.dev.json .",
    "format:check:prod": "biome format --config-path=biome.prod.json .",
    "check": "biome check --config-path=biome.dev.json .",
    "check:prod": "biome check --config-path=biome.prod.json --write .",
    "biome:ci": "biome ci --config-path=biome.prod.json ."
  },
  "dependencies": {
    "@hookform/resolvers": "^5.1.1",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@tailwindcss/vite": "^4.1.10",
    "@tanstack/react-query": "^5.81.2",
    "@tanstack/react-query-devtools": "^5.81.2",
    "@tanstack/react-router": "^1.121.12",
    "@tanstack/react-router-devtools": "^1.121.12",
    "@tanstack/router-core": "^1.131.36",
    "axios": "^1.10.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.18.1",
    "i18next": "^25.5.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "lucide-react": "^0.542.0",
    "next-themes": "^0.4.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.58.0",
    "react-i18next": "^15.7.3",
    "sonner": "^2.0.5",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.10",
    "tiny-invariant": "^1.3.3",
    "zod": "^4.1.5",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.4",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@tanstack/router-plugin": "^1.121.13",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.0.1",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^5.0.2",
    "@vitejs/plugin-react-swc": "^4.0.1",
    "@vitest/coverage-v8": "^3.2.4",
    "conventional-changelog-atom": "^5.0.0",
    "cross-env": "^10.0.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "tw-animate-css": "^1.3.4",
    "typescript": "^5.9.2",
    "vite": "^7.1.4",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.3"
  }
}
